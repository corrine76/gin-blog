    "editor.wordWrap": "on",
    "editor.fontSize": 13,
    "window.zoomLevel": 0,
    "go.docsTool": "gogetdoc",
    "go.formatTool": "goimports",
    "explorer.confirmDelete": false,
    "go.lintFlags": [
        "--disable=varcheck",
        "--enable=errcheck"
    ],
    "go.vetOnSave": "off"
}